@font-face {
	font-family:'SFUIDisplay';
	src: url('../fonts/SFUIDisplay-Regular_gdi.eot');
	src: url('../fonts/SFUIDisplay-Regular_gdi.eot?#iefix') format('embedded-opentype'),
		url('../fonts/SFUIDisplay-Regular_gdi.woff') format('woff'),
		url('../fonts/SFUIDisplay-Regular_gdi.ttf') format('truetype'),
		url('../fonts/SFUIDisplay-Regular_gdi.otf') format('opentype'),
		url('../fonts/SFUIDisplay-Regular_gdi.svg#SFUIDisplay') format('svg');
	font-weight: 400;
	font-style: normal;
	font-stretch: normal;
	unicode-range: U+0020-25CA;
}

